@article{Bisin2001,
	Annote = {Bisin and Verdier (2001) model the evolution of a cultural trait with vertical (i.e. ``socialization in the family'') and non-vertical (i.e. ``imitation from society'') transmission. Importantly, they assume populations with overlapping generations and do not distinguish between horizontal (from peer-to-peer) or oblique (between unrelated individuals from different generations) transmission. This assumption allows them to use a single differential equation (eq. 3). Interestingly, they focus on a frequency-dependent transmission mode that they call ``cultural substitution'', in which the parent invest more in child socialization if the parental trait is rare. They find that under such a transmission mode and in the absence of selection, a globally stable polymorphism exists (proposition 1); without frequency-dependence of the transmission rate, such a polymorphism is only neutrally stable (set s=0 in eq. 1 above). With selection, their model (eq. 12) is a continuous-time equivalent of our model (eq. 1), which considers only constant, rather than fluctuating, selection. However, by using more elaborate selection schemes, in which parents can choose the number of offspring, either an unstable polymorphism (proposition 4) or a stable polymorphism (proposition 5) exist, depending on the costs associated with the transmission modes.},
	Author = {Bisin, Alberto and Verdier, Thierry},
	Date-Modified = {2018-11-05 23:02:14 +0000},
	Doi = {10.1006/jeth.2000.2678},
	Journal = {Journal of Economic Theory},
	Link = {http://linkinghub.elsevier.com/retrieve/pii/S0022053100926784},
	Month = {apr},
	Pages = {298--319},
	Title = {The Economics of Cultural Transmission and the Dynamics of Preferences},
	Volume = {97},
	Year = {2001},
