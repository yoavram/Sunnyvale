@article{Ohtsuki2017,
	Annote = {models of cumulative culture, in which individual learners produce new cultural traits, which are then transmitted and accumulated via social learning. Several authors have suggested that the evolution of individual vs. social learning is similar to the evolution of cooperation. Under this view, social learners (the ``cheaters''), which benefit from new cultural information produced by individual learners (the ``cooperators'') without paying the cost of individual learning, ultimately causing the collapse of cumulative culture (Wakano, Miura 2014?). To resolve this, Ohtuski et al. (2017) demonstrated that if social learning is at least partially vertical, rather than horizontal or oblique, and if the population is structured such that kin-selection has a significant effect on the dynamics, then the inclusive fitness advantage of social learning can negate the cost of individual learning, at least to some extent, and allow for high levels of cumulative culture to evolve.},
	Author = {Ohtsuki, Hisashi and Wakano, Joe Yuichiro and Kobayashi, Yutaka},
	Doi = {10.1016/j.tpb.2017.03.001},
	Issn = {00405809},
